    "coffee-script": "^1.10.0",
    "mocha": "^2.2.5",
    "sinon": "^2.0.0-pre",
    "sinon-chai": "https://github.com/FredyC/sinon-chai/tarball/sinon2",
    "testem": "^0.9.11",
    "uglify-js": "^2.4.23",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "async": "^1.0.0",
    "debug": "^2.2.0",
    "fast.js": "^0.1.1",
