{
  "name": "scent",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlackDice/scent.git"
  },
  "main": "lib/scent.js",
  "license": "MIT",
  "scripts": {
    "compile": "coffee --no-header -bco lib/es6 src && babel -qd lib lib/es6",
